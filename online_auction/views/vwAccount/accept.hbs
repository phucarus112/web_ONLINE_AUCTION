    <section class="contact-section spad">
        <div class="container">
            <div class="row">
                <div class="product-tab">
                    <div class="tab-item">
                        <ul class="nav" role="tablist">
                            <li>
                                <a class="active" data-toggle="tab" href="#tab-1" role="tab">NÂNG CẤP</a>
                            </li>
                            <li>
                                <a data-toggle="tab" href="#tab-2" role="tab">HẠ CẤP</a>
                            </li>
                        </ul>
                    </div>

                    <div class="tab-item-content">
                        <div class="tab-content">
                            <div class="tab-pane fade-in active" id="tab-1" role="tabpanel">     
                              <div class="col-lg-12">
                                  <br>
                                    {{#if this.empty}}
                                    <div class="card-body">
				                        <p class="card-text">
					                        Không có yêu cầu nào cần xét duyệt
				                        </p>
			                        </div>
                                    {{else}}
                                        {{#each this.requireList}}
                                    <div class="cart-buttons">
                                        <button  type="button" data-toggle="modal" data-target="#exampleModalCenter"  href="#" class="text-left site-btn login-btn col-lg-12">@<b><i>{{USERNAME}}</i> </b> 
                                            đã gửi yêu cầu
                                        </button>
                                        <form action="" method = "POST" id= "{{ID}}">
                                            <input type="text" class="form-control" id="txtId" name="txtId" 
                                            style="display: none;" value="{{ID}}" readonly>
                                            <input type="text" class="form-control" id="temp" name="temp" value="0" 
                                            style="display: none;" readonly>
                                            <div class="group-input">
                                                <div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                                                    <div class="modal-dialog modal-dialog-centered" role="document">
                                                        <div class="modal-content">
                                                            <div class="modal-header">
                                                                <h5 class="modal-title" id="exampleModalCenterTitle">Bạn có muốn nâng cấp tài khoản này thành seller không</h5>
                                                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                                <span aria-hidden="true">&times;</span>
                                                                </button>
                                                            </div>
                                                    
                                                            <div class="modal-footer">
                                                                <button type="submit" class="btn btn-primary" onclick="assign1()">Chấp nhận</button>
                                                                <button type="submit" class="btn btn-primary" onclick="assign2()" >Từ chối</button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                    {{/each}}
                                {{/if}}         
                            </div>
                        </div>   
                   <div class="tab-pane fade-in " id="tab-2" role="tabpanel"> 
                       <br>   
                     <div class="col-lg-12">
                                  <br>
                                    {{#if this.emptySeller}}
                                    <div class="card-body">
				                        <p class="card-text">
					                        Không có seller nào
				                        </p>
			                        </div>
                                    {{else}}
                                        {{#each this.seller}}
                                    <div class="cart-buttons ">
                                        <form action="" method = "POST" id= "frm{{ID}}">
                                             <input type="text" class="form-control" id="txtId2" name="txtId2" 
                                            style="display: none;" value="{{ID}}" readonly>
                                            <button type="button" class="site-btn login-btn" >@<b><i>{{USERNAME}}</i></b></button>
                                            <a href="javascript: $('#frm{{ID}}').submit();"><i class="ti-close"></i></a>
                                        </form>
                                        </div>
                                    {{/each}}
                                {{/if}}         
                            </div>
                   </div>    
                           
                    </div>
 
        </div>
        </div>
        </div>
        </div>
{{#section 'js'}}

     
{{/section}}
    </section>
    <!-- Contact Section End -->