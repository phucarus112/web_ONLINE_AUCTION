    <!-- Shopping Cart Section Begin -->
    <section class="shopping-cart spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="cart-table">
                        
                                
                                    {{#if this.empty}}
                                        <div class="card-body">
				                            <p class="card-text">
					                           Không có sản phẩm nào
				                            </p>
			                            </div>
                                    {{else}}
                                    <table>
                            <thead>
                                <div class="card-body">
				                            <p class="card-text">
					                           Các sản phẩm đăng bán (còn thời hạn):
				                            </p>
			                            </div>
                                <tr>
                                    <th>Image</th>
                                    <th class="p-name">Product Name</th>
                                    <th>Current Price</th>
                                    <th>Turn Auction</th>
                                </tr>
                            </thead>
                            <tbody>
                                        {{#each this.selling}}
                                        <tr>
                                <td class="cart-pic first-row"><a href="/categories/detail/{{ID}}"><img src="/imgs/sp/{{ID}}/main_thumb.jpg" alt=""></a></td>
                                    <td class="cart-title first-row ">
                                        <h5>{{NAME}}</h5>
                                    </td>
                                    <td class="p-price first-row">{{format PRICE}}</td>
                                    <td class="total-price first-row">{{TURN_AUCTION}}</td>
                                        </tr>   
                                        {{/each}}
                                    {{/if}}
                            </tbody>
                        </table>
                    </div>
                
                </div>
            </div>
        </div>
    </section>
 